#include<iostream>
int main(){
    std::cout << "Hey Striver!" << "\n";
    // \n for next line.
    std::cout << "Hey Striver!" << std::endl;
    // endl is also used for leaving a line , hence works as  \n only.
    std::cout << "Hey Striver!" << std::endl << "Hey Hardik" << std::endl << "Hey Priyanshi";

    return 0;
}